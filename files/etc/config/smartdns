	option enabled '1'
	option name 'CN-ADV DNSPOD DOH'
	option ip 'https://doh.pub/dns-query'
	option type 'https'

config server
	option enabled '1'
	option name 'CN-ADV 360 DOH'
	option ip 'https://doh.360.cn/dns-query'
	option type 'https'

config server
	option enabled '1'
	option name 'CN-ADV rubyfish DOH'
	option ip 'https://rubyfish.cn/dns-query'
	option type 'https'

config server
	option name 'CN-ADV Cloudflare DOH'
	option ip 'https://1.1.1.1/dns-query'
	option type 'https'
	option enabled '1'

